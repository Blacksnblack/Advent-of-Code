	static String string = str1 + str2;
	static int total;
	
	public static void main(String args[]){
		for (int i = 0; i < string.length(); i++){
			if (string.charAt(i) == '(' ) {
				total++;
			}else if (string.charAt(i) == ')'){
				total--;
			}		
		}	
		System.out.println(total);
	}
}
