package Code3;

import java.io.IOException;

import org.jsoup.Jsoup;

public class code3 {

	public static void main(String args[]){
		org.jsoup.nodes.Document doc = null;
		try {
			doc = Jsoup.connect("http://adventofcode.com/day/2/input").get();
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		org.jsoup.select.Elements input = doc.select("word-wrap: break-word; white-space: pre-wrap");
		//The code above takes the text off the website
		
		
		
		
		
		String[] inp = input.text().split("");
		
		
		
		System.out
				.println(input);
		System.out
				.println(inp);
		
		
	}
}
