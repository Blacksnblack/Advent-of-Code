		static int total;
		
		
		public static void main(String args[]){
			total = 2;
			for (int i = 0; i < string.length(); i++){
				if (string.charAt(i) == '(' ) {
					total++;
				}else if (string.charAt(i) == ')'){
					total--;
				}		
				if (total <= -1){
					System.out.println(i);
				}
			}	
			System.out.println(total);
		}
